{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Lambda",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std",
      "console": "integratedTerminal",
      "runtimeArgs": [
        "-r",
        "dotenv/config"
      ],
      "program": "${workspaceFolder}/index.js"
    }
  ]
}